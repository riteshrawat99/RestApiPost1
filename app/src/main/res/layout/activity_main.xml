<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/nameText"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginBottom="76dp"
        android:background="@drawable/edit_desing"
        android:drawableStart="@drawable/baseline_person_24"
        android:drawablePadding="6dp"
        android:elevation="10dp"
        android:ems="10"
        android:fontFamily="@font/font9"
        android:hint="Enter title"
        android:inputType="textEmailAddress"
        android:padding="10dp"
        android:textColorHint="#B9B1B1"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.36" />

    <EditText
        android:id="@+id/emailText"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:layout_marginBottom="76dp"
        android:background="@drawable/edit_desing"
        android:drawableLeft="@drawable/baseline_email_24"
        android:drawablePadding="6dp"
        android:elevation="10dp"
        android:ems="10"
        android:fontFamily="@font/font9"
        android:hint="Enter body"
        android:inputType="text"
        android:padding="10dp"
        android:textColorHint="#B9B1B1"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/nameText"
        app:layout_constraintVertical_bias="0.06" />

    <EditText
        android:id="@+id/userId"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:background="@drawable/edit_desing"
        android:drawableLeft="@drawable/baseline_email_24"
        android:drawablePadding="6dp"
        android:elevation="10dp"
        android:ems="10"
        android:fontFamily="@font/font9"
        android:hint="Enter userId"
        android:inputType="text"
        android:padding="10dp"
        android:textColorHint="#B9B1B1"
        android:textSize="17sp"
        app:layout_constraintBottom_toTopOf="@+id/nameText"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.339" />

    <EditText
        android:id="@+id/id"
        android:layout_width="0dp"
        android:layout_height="44dp"
        android:background="@drawable/edit_desing"
        android:drawableLeft="@drawable/baseline_email_24"
        android:drawablePadding="6dp"
        android:elevation="10dp"
        android:ems="10"
        android:fontFamily="@font/font9"
        android:hint="Enter id"
        android:inputType="text"
        android:padding="10dp"
        android:textColorHint="#B9B1B1"
        android:textSize="17sp"

        app:layout_constraintBottom_toTopOf="@+id/nameText"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/userId"
        app:layout_constraintVertical_bias="0.39" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="16dp" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="632dp"
        android:layout_height="280dp"
        android:rotationX="-12"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.755"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.037"
        app:srcCompat="@drawable/wave__3_" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/submitBtn"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/button_desigb"
        android:elevation="10dp"
        android:fontFamily="@font/font3"
        android:text="Submit"
        android:textAllCaps="false"
        android:textColor="#000"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toBottomOf="@+id/emailText"
        app:layout_constraintVertical_bias="0.0" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_end="16dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:elevation="10dp"
        android:layout_marginTop="96dp"
        android:padding="1dp"
        android:background="@drawable/circle_image"
        android:clipToOutline="true"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toTopOf="@+id/emailText"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.371"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.158"
        app:srcCompat="@drawable/apiimage"
        tools:ignore="MissingConstraints"
        tools:visibility="visible" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="92dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline2"
        app:layout_constraintHorizontal_bias="0.047"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/submitBtn"
        android:textSize="20sp"
        android:fontFamily="@font/font9"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>